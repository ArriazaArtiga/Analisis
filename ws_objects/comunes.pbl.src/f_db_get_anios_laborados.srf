global function integer f_db_get_anios_laborados (string as_dpi, long al_no_solicitud);Integer li_anio

Select anio 
into :li_anio
from Solicitud_Tiempo_Servicio 
where No_Solicitud = :al_no_solicitud 
and DPI = :as_dpi
using SQLCA;

Return li_anio
end function


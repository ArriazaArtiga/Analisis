global function string f_db_get_solicitud_auxilio_postumo (string as_dpi);String ls_auxpost

select 	b.AuxPost
into 		:ls_auxpost
from		solicitudes b
where	b.DPI = :as_dpi and b.etapa <210 
using SQLCA;

return ls_auxpost
end function


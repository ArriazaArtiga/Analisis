global function string f_db_get_usuario_analista (long al_idanalista);String ls_analista

select  	b.usuario 
into 		:ls_analista
from		Ejecutivos_Analisis b
where   	b.idEjecutivo = :al_idanalista
using 		SQLCA;

Return ls_analista
end function

